<!DOCTYPE html>
<html>
  <head>
    <title>Observationalrails</title>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>

    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
  </head>

  <body>
  <nav>
    <%= link_to "Planets", planets_path, class: "dismyclass" %>
    <%= link_to "Systems", systems_path %>
    <%= link_to "Galaxies", galaxies_path %>
    <%= link_to "Observations", observations_path %>
  </nav>
    <%= yield %>
  </body>
</html>
